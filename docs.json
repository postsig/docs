{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#18162d",
    "light": "#ffffff",
    "dark": "#18162d"
  },
  "favicon": "/favicon.svg",
  "fonts": {
    "heading": {
      "family": "FK Grotesk",
      "source": "/fonts/FKGrotesk-Medium.woff2",
      "format": "woff2",
      "weight": 500
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "fields", "contract-types"]
          }
        ]
      }
    ],
    "global": {}
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@postsig.com.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Login",
      "href": "https://app.postsig.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
